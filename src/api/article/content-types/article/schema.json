      "allowedTypes": [
        "images"
      ]
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "videos"
      ]
    },
    "topic": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::topic.topic"
    }
  }
